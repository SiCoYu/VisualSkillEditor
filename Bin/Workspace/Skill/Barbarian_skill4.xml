        <action id="playanim" animid="%0" isautodel="1"/>
        <action id="addimpact" impactid="%1" targettype="2" isautodel="1"/>
        <action id="wait" time="300"/>
        <action id="addimpact" impactid="%1" targettype="2" isautodel="1"/>
        <action id="wait" time="300"/>
        <action id="addimpact" impactid="%1" targettype="2" isautodel="1"/>
        <action id="wait" time="300"/>
